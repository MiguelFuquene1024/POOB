import java.time.LocalDateTime;
import java.util.HashMap;
/**
	 dado el nombre del problema se busca en todos los problemas
	 @param problemName, el nombre del problema a buscar el cual es tipo String
	 @return Problem
	*/

public class Contest{
	private String name;
	private LocalDateTime start;
	private LocalDateTime end;
	private HashMap< String, Team> enrolled;
	private HashMap< String, Problem> problems;
	
	public Problem findProblem( String problemName ){
		private Problem problem = null ;
		if ( problemns.iskey( problemName)){
			problem = problemns.get( problemName);
		}
		return problem;
		
	}
	
}